{
	"name": "Network Requests Monitor",
	"version": "2.3",
	"description": "Network Requests for current Tab",
	"devtools_page": "networkrequests.html",
	"manifest_version": 2,
	"web_accessible_resources": ["*"],
	"permissions": [
		"tabs",
		"clipboardWrite",
		"http://*/*",
		"https://*/*",
		"webRequest"
	],
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	}
}